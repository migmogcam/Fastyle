-- 0 +/- SELECT COUNT(*) FROM PUBLIC.ESTETICISTA_SERVICIOS_ESTETICOS;         
CREATE MEMORY TABLE "PUBLIC"."SERVICIO_ESTETICO"(
    "ID" INTEGER DEFAULT NEXT VALUE FOR "PUBLIC"."SYSTEM_SEQUENCE_0CC3716A_4979_4D15_B628_4F71CD4C49AD" NOT NULL NULL_TO_DEFAULT SEQUENCE "PUBLIC"."SYSTEM_SEQUENCE_0CC3716A_4979_4D15_B628_4F71CD4C49AD",
    "CIUDAD" VARCHAR(100),
    "DETALLE" VARCHAR(400),
    "DETALLE_ACORTADO" VARCHAR(200),
    "IMAGEN" VARCHAR(255),
    "PRECIO" DOUBLE CHECK (("PRECIO" >= 0.0)
    AND ("PRECIO" <= 9.223372036854776E+18)),
    "PROVINCIA" VARCHAR(100),
    "TIPO" VARCHAR(100)
);             
