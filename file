CREATE MEMORY TABLE "PUBLIC"."ADMINISTRADOR"(
    "ID" INTEGER NOT NULL,
    "APELLIDO1" VARCHAR(100),
    "APELLIDO2" VARCHAR(100),
    "CIUDAD" VARCHAR(100),
    "E_MAIL" VARCHAR(100),
    "NAME" VARCHAR(100),
    "PASSWORD" VARCHAR(15),
    "PROVINCIA" VARCHAR(100),
    "USUARIO" VARCHAR(15)
);          
ALTER TABLE "PUBLIC"."ADMINISTRADOR" ADD CONSTRAINT "PUBLIC"."CONSTRAINT_9" PRIMARY KEY("ID");
-- 0 +/- SELECT COUNT(*) FROM PUBLIC.ADMINISTRADOR;           
CREATE MEMORY TABLE "PUBLIC"."AUTHORITIES"(
    "USERNAME" VARCHAR(255) NOT NULL,
    "AUTHORITY" VARCHAR(255)
);          
ALTER TABLE "PUBLIC"."AUTHORITIES" ADD CONSTRAINT "PUBLIC"."CONSTRAINT_A" PRIMARY KEY("USERNAME");            
-- 0 +/- SELECT COUNT(*) FROM PUBLIC.AUTHORITIES;             
CREATE MEMORY TABLE "PUBLIC"."CITA"(
    "ID" INTEGER DEFAULT NEXT VALUE FOR "PUBLIC"."SYSTEM_SEQUENCE_76D11FB1_FCE3_4558_A6A7_6B09A3996AC5" NOT NULL NULL_TO_DEFAULT SEQUENCE "PUBLIC"."SYSTEM_SEQUENCE_76D11FB1_FCE3_4558_A6A7_6B09A3996AC5",
    "DETALLE" VARCHAR(250),
    "FECHA" DATE,
    "HORA" INTEGER CHECK (("HORA" >= 0)
    AND ("HORA" <= 24)),
    "MINUTO" INTEGER CHECK (("MINUTO" >= 0)
    AND ("MINUTO" <= 60)),
    "USUARIO" INTEGER
);       
ALTER TABLE "PUBLIC"."CITA" ADD CONSTRAINT "PUBLIC"."CONSTRAINT_1" PRIMARY KEY("ID");         
-- 0 +/- SELECT COUNT(*) FROM PUBLIC.CITA;    
